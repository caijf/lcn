{
  "name": "lcn",
  "version": "3.0.1",
  "description": "中华人民共和国行政区划，省市区数据",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "types/src/index.d.ts",
  "scripts": {
    "build": "npm run build:npm && npm run build:types",
    "build:npm": "rm -rf dist && rollup -c",
    "build:data": "rm -rf data && node scripts/index.js",
    "build:types": "rm -rf types && tsc --outDir types",
    "test": "jest --verbose",
    "precommit": "lint-staged",
    "commit": "cz"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "files": [
    "data",
    "dist",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caijf/lcn.git"
  },
  "author": "caijf",
  "license": "MIT",
  "keywords": [
    "lcn",
    "location-china",
    "china-location",
    "location-area",
    "china-area"
  ],
  "bugs": {
    "url": "https://github.com/caijf/lcn/issues"
  },
  "homepage": "https://github.com/caijf/lcn#readme",
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "babel-jest": "^27.0.1",
    "cheerio": "^1.0.0-rc.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.27.0",
    "husky": "4",
    "iconv-lite": "^0.6.2",
    "jest": "^27.0.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "rollup": "^2.50.0",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.2.4"
  }
}