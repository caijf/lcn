{
  "name": "lcn",
  "version": "4.1.7",
  "description": "中华人民共和国行政区划，省市区数据",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "types/src/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && npm run build:module && npm run build:umd && npm run build:types",
    "build:module": "rollup -c rollup.module.config.mjs",
    "build:umd": "rollup -c rollup.umd.config.mjs",
    "build:data": "rm -rf data && node scripts/index.js",
    "build:types": "rm -rf types &&  tsc -p tsconfig.build.json -d --emitDeclarationOnly --outDir types",
    "test": "jest --verbose",
    "commit": "cz",
    "tsc": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm test",
    "prepare": "husky install"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "files": [
    "data",
    "dist",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caijf/lcn.git"
  },
  "author": "caijf",
  "license": "MIT",
  "keywords": [
    "lcn",
    "location-china",
    "china-location",
    "location-area",
    "china-area"
  ],
  "bugs": {
    "url": "https://github.com/caijf/lcn/issues"
  },
  "homepage": "https://github.com/caijf/lcn#readme",
  "devDependencies": {
    "@babel/core": "^7.22.20",
    "@babel/preset-env": "^7.22.20",
    "@babel/preset-typescript": "^7.22.15",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@rollup/plugin-commonjs": "^25.0.4",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.2.1",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.3",
    "@types/jest": "^29.5.5",
    "@typescript-eslint/eslint-plugin": "^6.7.2",
    "@typescript-eslint/parser": "^6.7.2",
    "babel-jest": "^29.7.0",
    "cheerio": "^1.0.0-rc.12",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.49.0",
    "husky": "^8.0.3",
    "iconv-lite": "^0.6.3",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "rollup": "^3.29.2",
    "typescript": "^5.2.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest-4"
  },
  "dependencies": {
    "tslib": "^2.6.2"
  }
}
